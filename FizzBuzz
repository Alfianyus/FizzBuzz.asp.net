using System;

namespace FizzBazz1
{
    class Program
    {
        static void Main(string[] args)
        {
          for(int j = 1; j <= 50; j++)
            {
                string results = "";

                if (j % 3 == 0) results = "Fizz";

                if (j % 5 == 0) results = results + "Buzz";

                if (results.Length == 0) results = j.ToString();
                {
                    Console.WriteLine(results);
                }
            }
        }
    }
}
